---
import '@fontsource/jetbrains-mono/variable.css';

// Nunito
// Dosis
---

<style is:inline is:global>
  :root {
    --aw-font-sans: 'JetBrains MonoVariable', monospace;
    --aw-font-serif: var(--aw-font-sans);
    --aw-font-heading: var(--aw-font-sans);

    --aw-color-primary: rgb(144, 0, 72);
    --aw-color-secondary: rgb(199, 155, 255);
    --aw-color-accent: rgb(0, 255, 183);
    --aw-color-text-page: rgb(25, 0, 46);
    --aw-color-text-muted: rgb(75 85 99);
    --aw-color-bg-page: rgb(231, 212, 255);
    --wb-bg-dark: rgb(36, 0, 65);
    --wb-bg-darker: rgb(25, 0, 46);
    --wb-green: rgb(0, 255, 183);
    --wb-cherry: rgb(144, 0, 72);
    --wb-pink: rgb(255, 14, 130);
    --wb-purple: rgb(199, 155, 255);
  }
</style>
