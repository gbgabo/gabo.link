    "@iconify-json/ep": "^1.1.14",
    "@iconify-json/eva": "^1.1.10",
    "@iconify-json/fluent": "^1.1.45",
    "@iconify-json/ic": "^1.1.17",
    "@iconify-json/material-symbols": "^1.1.72",
    "@iconify-json/mdi": "^1.1.63",
